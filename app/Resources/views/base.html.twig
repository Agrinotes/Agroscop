    <meta name="description" content="Agroscopik">
    <meta name="author" content="Hugo Lehoux">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" type="shortcut icon" href="{{ asset('favicon.ico') }}"/>
    <link rel="apple-touch-icon" href="{{ asset('front/mmenu/assets/images/apple-touch-icon.png') }}">

    {% block stylesheets %}
        {# Bootstrap and site stylesheets #}
        <link rel="stylesheet" href="{{ asset('front/global/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('front/global/css/bootstrap-extend.min.css') }}">
        <link rel="stylesheet" href="{{ asset('front/mmenu/assets/css/site.css') }}">
        {# Necessary plugin stylesheets #}
        {% block plugin_stylesheets %}
            <link rel="stylesheet" href="{{ asset('front/global/vendor/animsition/animsition.css') }}">
            <link rel="stylesheet" href="{{ asset('front/global/vendor/asscrollable/asScrollable.css') }}">
            <link rel="stylesheet" href="{{ asset('front/global/vendor/switchery/switchery.css') }}">
            <link rel="stylesheet" href="{{ asset('front/global/vendor/slidepanel/slidePanel.css') }}">
            <link rel="stylesheet" href="{{ asset('front/global/vendor/jquery-mmenu/jquery-mmenu.css') }}">
            <link rel="stylesheet" href="{{ asset('front/global/vendor/flag-icon-css/flag-icon.css') }}">
            <link rel="stylesheet" href="{{ asset('front/global/vendor/waves/waves.css') }}">
        {% endblock %}
        {# Font stylesheets #}
        <link rel="stylesheet" href="{{ asset('front/global/fonts/font-awesome/font-awesome.css') }}">
        <link rel="stylesheet" href="{{ asset('front/global/fonts/material-design/material-design.min.css') }}">
        <link rel="stylesheet" href="{{ asset('front/global/fonts/web-icons/web-icons.min.css') }}">
        <link rel="stylesheet" href="{{ asset('front/global/fonts/brand-icons/brand-icons.min.css') }}">
        <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,600,300italic'>
    {% endblock %}

    {% block browser_compatibility %}
        <!--[if lt IE 9]>
        <script src="{{ asset('front/global/vendor/html5shiv/html5shiv.min.js') }}"></script>
        <![endif]-->
        <!--[if lt IE 10]>
        <script src="{{ asset('front/global/vendor/media-match/media.match.min.js') }}"></script>
        <script src="{{ asset('front/global/vendor/respond/respond.min.js') }}"></script>
        <![endif]-->
    {% endblock %}

    {% block head_javascripts %}
        <script src="{{ asset('front/global/vendor/modernizr/modernizr.js') }}"></script>
        <script src="{{ asset('front/global/vendor/breakpoints/breakpoints.js') }}"></script>
        <script>
            Breakpoints();
        </script>
    {% endblock %}

</head>

<body class="{% block body_class %}site-navbar-small site-menubar-unfold site-menubar-keep{% endblock %}">


<nav class="site-navbar navbar navbar-inverse navbar-fixed-top navbar-mega bg-red-600" role="navigation">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle hamburger hamburger-close navbar-toggle-left hided"
                data-toggle="menubar">
            <span class="sr-only">Toggle navigation</span>
            <span class="hamburger-bar"></span>
        </button>
        <button type="button" class="navbar-toggle collapsed" data-target="#site-navbar-collapse"
                data-toggle="collapse">
            <i class="icon md-more" aria-hidden="true"></i>
        </button>
        <div class="navbar-brand navbar-brand-center site-gridmenu-toggle" data-toggle="gridmenu">
            <img class="navbar-brand-logo" src="{{ asset('front/mmenu/assets/images/logo.png') }}" title="Agroscopik">
            <span class="navbar-brand-text hidden-xs"> AGROSCOPIK</span>
        </div>
    </div>
    <div class="navbar-container container-fluid">
        <!-- Navbar Collapse -->
        <div class="collapse navbar-collapse navbar-collapse-toolbar" id="site-navbar-collapse">
            <!-- Navbar Toolbar -->
            <ul class="nav navbar-toolbar">
                <li class="hidden-float" id="toggleMenubar">
                    <a data-toggle="menubar" href="#" role="button">
                        <i class="icon hamburger hamburger-arrow-left">
                            <span class="sr-only">Toggle menubar</span>
                            <span class="hamburger-bar"></span>
                        </i>
                    </a>
                </li>
            </ul>
            <!-- End Navbar Toolbar -->
            <!-- Navbar Toolbar Right -->
            <ul class="nav navbar-toolbar navbar-right navbar-toolbar-right">
                <li class="dropdown">
                    <a class="navbar-avatar dropdown-toggle" data-toggle="dropdown" href="#" aria-expanded="false"
                       data-animation="scale-up" role="button">
              <span class="avatar avatar-online">
                <img src="{{ asset('front/global/portraits/5.jpg') }}" alt="Photo_de_profil">
                <i></i>
              </span>
                    </a>
                    <ul class="dropdown-menu" role="menu">
                        <li role="presentation">
                            <a href="javascript:void(0)" role="menuitem"><i class="icon md-account"
                                                                            aria-hidden="true"></i> Mon profil</a>
                        </li>
                        <li role="presentation">
                            <a href="javascript:void(0)" role="menuitem"><i class="icon md-settings"
                                                                            aria-hidden="true"></i> Paramètres</a>
                        </li>
                        <li class="divider" role="presentation"></li>
                        <li role="presentation">
                            <a href="{{ path('fos_user_security_logout') }}" role="menuitem"><i class="icon md-power"
                                                                                                aria-hidden="true"></i>
                                Déconnexion</a>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- End Navbar Toolbar Right -->
        </div>
        <!-- End Navbar Collapse -->
    </div>
</nav>

