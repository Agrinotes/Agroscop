    {# Core and plugin dependencies #}
{% block core_plugin %}
    <script src="{{ asset('front/global/vendor/jquery/jquery.js') }}"></script>
    <script src="{{ asset('front/global/vendor/bootstrap/bootstrap.js') }}"></script>
    <script src="{{ asset('front/global/vendor/animsition/animsition.js') }}"></script>
    <script src="{{ asset('front/global/vendor/asscroll/jquery-asScroll.js') }}"></script>
    <script src="{{ asset('front/global/vendor/mousewheel/jquery.mousewheel.js') }}"></script>
    <script src="{{ asset('front/global/vendor/asscrollable/jquery.asScrollable.all.js') }}"></script>
    <script src="{{ asset('front/global/vendor/ashoverscroll/jquery-asHoverScroll.js') }}"></script>
    <script src="{{ asset('front/global/vendor/waves/waves.js') }}"></script>
    <script src="{{ asset('front/global/vendor/jquery-mmenu/jquery.mmenu.min.all.js') }}"></script>
    <script src="{{ asset('front/global/vendor/switchery/switchery.min.js') }}"></script>
    <script src="{{ asset('front/global/vendor/intro-js/intro.js') }}"></script>
